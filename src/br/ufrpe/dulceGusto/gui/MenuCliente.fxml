<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <!-- TODO Add Nodes -->
  <children>
    <AnchorPane fx:id="apTelaCliente" layoutX="0.0" layoutY="0.0" prefHeight="400.0" prefWidth="600.0">
      <children>
        <ImageView id="imFundoCliente" fx:id="imgFundoCliente" fitHeight="400.0" fitWidth="600.0" layoutX="1.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@../fundo_doces_2.png" />
          </image>
        </ImageView>
        <MenuBar id="mbMenuOpçoes" fx:id="mbMenuCliente" layoutX="0.0" layoutY="0.0" prefWidth="601.0">
          <menus>
            <Menu mnemonicParsing="false" text="Novo pedido" fx:id="mnNovoPedido" />
            <Menu mnemonicParsing="false" text="Alterar informações" fx:id="mnAlterar">
              <items>
                <MenuItem mnemonicParsing="false" text="Alterar cadastro" fx:id="mnitAlterarCadastro" />
                <MenuItem mnemonicParsing="false" text="Alterar pedido" fx:id="mnitAlterarPedido" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Sair" fx:id="mnSair" />
          </menus>
        </MenuBar>
      </children>
    </AnchorPane>
  </children>
</AnchorPane>
